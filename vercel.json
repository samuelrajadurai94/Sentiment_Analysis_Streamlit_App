{
    "version": 2,
    "builds": [
        {"src": "streamlit_app_deployed_server.py", "use": "@vercel/python"}
    ],
    "routes": [
        {"src": "/(.*)", "dest": "/streamlit_app_deployed_server.py"}
    ]
    "functions": {
        "api/**": {
            "excludeFiles": "{.next,*.cache,node_modules,public,app}/**"
        }
    }
}
